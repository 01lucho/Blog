<h1>Posts</h1>


<table>
<% @posts.each do |post| %>
  <tr>
    <td><%= link_to post.title, post %></td>
  </tr>
  <tr>
    <td>by <%= post.user.nombre_completo %> </td>
  </tr>
  <tr>
    <td><%= time_ago_in_words(post.created_at) + " ago" %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Post', new_post_path %>
