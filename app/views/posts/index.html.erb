<h1>Posts</h1>


<table>
<% @posts.each do |post| %>
  <tr>
    <td><%= post.title %></td>
    <td><%= post.created_at.strftime("%d %b. %Y %H:%M") %></td>
    <td><%= post.user.nombre_completo %></td>
    <td><%= link_to 'Show', post %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Post', new_post_path %>
